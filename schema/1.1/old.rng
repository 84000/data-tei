   <define name="p">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="p">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(paragraph) marks paragraphs in prose. [3.1. Paragraphs 7.2.5. Speech Contents]</a:documentation>
         <rng:ref name="macro.paraContent"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="NewODD-p-abstractModel-structure-p-in-ab-or-p-constraint-report-1">
            <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron" context="tei:p">
               <sch:report xmlns="http://www.tei-c.org/ns/1.0" test="(ancestor::tei:ab or ancestor::tei:p) and not( ancestor::tei:floatingText |parent::tei:exemplum |parent::tei:item |parent::tei:note |parent::tei:q |parent::tei:quote |parent::tei:remarks |parent::tei:said |parent::tei:sp |parent::tei:stage |parent::tei:cell |parent::tei:figure )">
        Abstract model violation: Paragraphs may not occur inside other paragraphs or ab elements.
      </sch:report>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="NewODD-p-abstractModel-structure-p-in-l-or-lg-constraint-report-2">
            <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron" context="tei:p">
               <sch:report xmlns="http://www.tei-c.org/ns/1.0" test="(ancestor::tei:l or ancestor::tei:lg) and not( ancestor::tei:floatingText |parent::tei:figure |parent::tei:note )">
        Abstract model violation: Lines may not contain higher-level structural elements such as div, p, or ab, unless p is a child of figure or note, or is a descendant of floatingText.
      </sch:report>
            </sch:rule>
         </pattern>
         <optional>
            <attribute name="tid">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Numbers paragraph and line groups in a particular text.</a:documentation>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="key">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Designates a paragraph to only be applicable and only be displayed in one text version according to the key defined in the <code xmlns="http://www.w3.org/1999/xhtml">&lt;sourceDesc&gt;</code>.</a:documentation>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <rng:choice>
                  <rng:value>tantricRestriction</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">This paragraph warns the reader that this text has a tantric restrition. Thie <code xmlns="http://www.w3.org/1999/xhtml">&lt;p&gt;</code> should be stated in the text's <code xmlns="http://www.w3.org/1999/xhtml">&lt;availability&gt;</code> in the <code xmlns="http://www.w3.org/1999/xhtml">&lt;publicationStmt&gt;</code>
                  </a:documentation>
                  <rng:value>sponsor</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">This paragraph details the sponsor for the publication of the text, and this should be placed in the "acknowledgements" found int he front of the text.</a:documentation>
               </rng:choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="rend">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <rng:choice>
                  <rng:value>mantra</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">This <code xmlns="http://www.w3.org/1999/xhtml">&lt;p&gt;</code> element contains a mantra. The mantra has been formally and clearly declared and so will set apart with indentation and vertical spacing in the 84000 web display. Not all <code xmlns="http://www.w3.org/1999/xhtml">&lt;p&gt;</code> elements that contain mantras need to be rendered in this way, but generally we do so, when they are formally and clearly declared in the text.</a:documentation>
                  <rng:value>ignoreGlossary</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Glossary terms found in this paragraph will not be linked to the glossary in the 84000 Reading Room.</a:documentation>
               </rng:choice>
            </attribute>
         </optional>
         <empty/>
      </rng:element>
   </define>
   <define name="foreign">
