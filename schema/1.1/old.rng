   <define name="table">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="table">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(table) contains text displayed in tabular form, in rows and columns. [14.1.1. TEI Tables]</a:documentation>
         <group>
            <rng:zeroOrMore>
               <rng:choice>
                  <rng:ref name="model.headLike"/>
                  <rng:ref name="model.global"/>
               </rng:choice>
            </rng:zeroOrMore>
            <rng:choice>
               <oneOrMore>
                  <group>
                     <rng:ref name="row"/>
                     <rng:zeroOrMore>
                        <rng:ref name="model.global"/>
                     </rng:zeroOrMore>
                  </group>
               </oneOrMore>
               <oneOrMore>
                  <group>
                     <rng:ref name="model.graphicLike"/>
                     <rng:zeroOrMore>
                        <rng:ref name="model.global"/>
                     </rng:zeroOrMore>
                  </group>
               </oneOrMore>
            </rng:choice>
            <rng:zeroOrMore>
               <group>
                  <rng:ref name="model.divBottom"/>
                  <rng:zeroOrMore>
                     <rng:ref name="model.global"/>
                  </rng:zeroOrMore>
               </group>
            </rng:zeroOrMore>
         </group>
         <rng:ref name="att.typed.attributes"/>
         <optional>
            <attribute name="tid">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Numbers the table in the text along with and in the same manner as paragraph, <code xmlns="http://www.w3.org/1999/xhtml">&lt;p&gt;</code>; and line group, <code xmlns="http://www.w3.org/1999/xhtml">&lt;l&gt;</code>, elements.</a:documentation>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="rend">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Modifies table's display in the HTML</a:documentation>
               <rng:choice>
                  <rng:value>firstcol-30pc</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Renders the first column with 30% width</a:documentation>
                  <rng:value>firstcol-40pc</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Renders the first column with 40% width</a:documentation>
                  <rng:value>firstcol-50pc</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Renders the first column with 50% width</a:documentation>
               </rng:choice>
            </attribute>
         </optional>
         <empty/>
      </rng:element>
   </define>
   <define name="row">
