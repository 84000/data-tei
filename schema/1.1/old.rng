               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Hindi written in Roman script</a:documentation>
               <rng:value>Ja-Ltn</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Japanese written in Roman script</a:documentation>
               <rng:value>Kn-Ltn</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Kannada written in Roman script</a:documentation>
               <rng:value>Mr-Ltn</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Marathi written in Roman script</a:documentation>
               <rng:value>Mn-Ltn</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Mongolian written in Roman script</a:documentation>
               <rng:value>Pi-Ltn</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Pali written in Roman script</a:documentation>
               <rng:value>Pra-Ltn</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Prakrit written in Roman script</a:documentation>
               <rng:value>Sa-Ltn</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Sanskrit written in Roman script (Note: the term will be italicized. do not tag this for terms that are left unitalicized in 84000 texts like sutra, nagas, dharma, Sanskrit names, etc.)</a:documentation>
               <rng:value>Ta-Ltn</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Tamil written in Roman script</a:documentation>
               <rng:value>Th-Ltn</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Thai written in Roman script</a:documentation>
               <rng:value>Zh-Ltn</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Chinese written in Roman script</a:documentation>
               <rng:value>Mn-Ltn</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Mongolian written in Roman script</a:documentation>
               <rng:value>Ko-Ltn</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Korean written in Roman script</a:documentation>
               <rng:value>Ar-Ltn</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Arabic written in Roman script</a:documentation>
               <rng:value>Fa-Ltn</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Persian written in Roman script</a:documentation>
               <rng:value>bo</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Tibetan in Tibetan script (Use Bo-Ltn if in Roman script or Wylie) </a:documentation>
               <rng:value>cs</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Czech</a:documentation>
               <rng:value>de</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">German (Deutsch)</a:documentation>
               <rng:value>el</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Greek</a:documentation>
               <rng:value>en</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">English</a:documentation>
               <rng:value>fr</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">French</a:documentation>
               <rng:value>it</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Italian</a:documentation>
               <rng:value>ja</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Japanese in Japanese script (Use Ja-Ltn if in Roman script)</a:documentation>
               <rng:value>la</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Latin</a:documentation>
               <rng:value>nl</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Dutch; Flemish</a:documentation>
               <rng:value>ru</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Russian</a:documentation>
               <rng:value>da</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Danish</a:documentation>
               <rng:value>pt</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Portuguese</a:documentation>
               <rng:value>es</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Spanish</a:documentation>
               <rng:value>sv</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Swedish</a:documentation>
               <rng:value>no</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Norwegian</a:documentation>
               <rng:value>po</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Polish</a:documentation>
               <rng:value>zh</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Chinese in Chinese script (Use Zh-Ltn if in Roman script)</a:documentation>
               <rng:value>middle_indic-Ltn</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Middle Indic written in Roman script.</a:documentation>
               <rng:value>saka-Ltn</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Saka written in Roman script.</a:documentation>
               <rng:value>sogdian-Ltn</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Sogdian written in Roman script.</a:documentation>
               <rng:value>mixed</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">This element contains strings mixed with different languages. They will be differentiated within this element by <code xmlns="http://www.w3.org/1999/xhtml">&lt;foreign&gt;</code> tags, or left unwrapped if they are the default English.</a:documentation>
            </rng:choice>
         </attribute>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Identifies a word or phrase as belonging to some language other than that of the surrounding text. Denotes also that the word or phrase in question will appear in italics. The full tag includes the xml:lang attribute with the appropriate language value and also the type attribute with <code xmlns="http://www.w3.org/1999/xhtml">@term</code>, <code xmlns="http://www.w3.org/1999/xhtml">@quote</code>, or <code xmlns="http://www.w3.org/1999/xhtml">@mantra</code> as a value.</a:documentation>
               <rng:choice>
                  <rng:value>quote</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Any untranslated quotations taken from the source text (the translator will typically put these in the endnotes).“Reverend Ānanda, do not bring shame upon us, but go in silence, lest the outsider sectarians should hear your words.” (Endnote−Tib. gzhan mu stegs can spyod pa pa dang kun tu rgyu dang gcer bu pa dang tsho ba pa dag; Skt. anyatīrthika caraka parivrajaka nigranthājīvāḥ. I have simply rendered this “outsider sectarians” so as not to burden the reader with irrelevant names, as this expression is a cliché for all the outsider groups occurring in other sūtras.)</a:documentation>
                  <rng:value>term</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A foreign term in parenthesis following its translation or a relatively rare foreign term left un-translated. Foreign terms also include short phrases that constitute technical or commonly used terms.The mind is immaterial, invisible, baseless, and unconscious (Tib. rnam par rig pa med pa; Skt. avijñapti; lit. “without means of cognition”).</a:documentation>
               </rng:choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="key">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <text/>
            </attribute>
         </optional>
         <empty/>
      </rng:element>
   </define>
   <define name="emph">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="emph">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(emphasized) marks words or phrases which are stressed or emphasized for linguistic or rhetorical effect. [3.3.2.2. Emphatic Words and Phrases 3.3.2. Emphasis, Foreign Words, and Unusual Language]</a:documentation>
         <oneOrMore>
            <rng:choice>
               <text/>
               <rng:ref name="term"/>
            </rng:choice>
         </oneOrMore>
         <optional>
            <attribute name="rend">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <text/>
            </attribute>
         </optional>
         <empty/>
      </rng:element>
   </define>
   <define name="hi">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="hi">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(highlighted) marks a word or phrase as graphically distinct from the surrounding text, for reasons concerning which no claim is made. [3.3.2.2. Emphatic Words and Phrases 3.3.2. Emphasis, Foreign Words, and Unusual Language]</a:documentation>
         <text/>
         <rng:ref name="att.global.attribute.xmllang"/>
         <rng:ref name="att.global.attribute.xmlid"/>
         <attribute name="rend">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <rng:choice>
               <rng:value>small-caps</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <rng:value>subscript</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <rng:value>superscript</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            </rng:choice>
         </attribute>
         <empty/>
      </rng:element>
   </define>
   <define name="distinct">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="distinct">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">identifies any word or phrase which is regarded as linguistically distinct, for example as archaic, technical, dialectal, non-preferred, etc., or as forming part of a sublanguage. [3.3.2.3. Other Linguistically Distinct Material]</a:documentation>
         <rng:ref name="macro.phraseSeq"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies the sublanguage or register to which the word or phrase is being assigned</a:documentation>
               <data type="token">
                  <rng:param name="pattern">[^\p{C}\p{Z}]+</rng:param>
               </data>
            </attribute>
         </optional>
         <empty/>
      </rng:element>
   </define>
   <define name="q">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="q">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(quoted) contains material which is distinguished from the surrounding text as a block quote [3.3.3. Quotation]</a:documentation>
         <rng:zeroOrMore>
            <rng:choice>
               <rng:ref name="macro.specialPara"/>
               <rng:ref name="orig"/>
            </rng:choice>
         </rng:zeroOrMore>
         <optional>
            <attribute name="source">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Currently used in knowledgebase files to identify the milestone of a quotation from a separate translation file. Eventually we will workout how to pull the text so if the translation gets updated we won't need to update the knowledgebase article.</a:documentation>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="ref">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Points to the id of the quoted passage. Typically we use this to cross-reference a commentary to it's root text source or a subsidiary text if applicable.</a:documentation>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="alt">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Identifies the alternate form of the quote as it appears in the root text when the commentary quote is different. The parser will look for this alternate string when searching to highlight the substring in the root text from the pop up window. Note that by default, the parser will ingnore quotation marks and some punctuation.</a:documentation>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="xml:id">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(type) may be used to indicate whether the offset passage is spoken or thought, or to characterize it more finely.</a:documentation>
               <rng:choice>
                  <rng:value>root-text</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>subsidiary-text</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>substring</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               </rng:choice>
            </attribute>
         </optional>
         <empty/>
      </rng:element>
   </define>
   <define name="desc">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="desc">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(description) contains a short description of the purpose, function, or use of its parent element, or when the parent is a documentation element, describes or defines the object being documented.  [22.4.1. Description of Components]</a:documentation>
         <rng:ref name="macro.limitedContent"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="NewODD-desc-deprecationInfo-only-in-deprecated-constraint-rule-3">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" context="tei:desc[ @type eq 'deprecationInfo']">
               <sch:assert test="../@validUntil">Information about a
        deprecation should only be present in a specification element
        that is being deprecated: that is, only an element that has a
        @validUntil attribute should have a child &lt;desc
        type="deprecationInfo"&gt;.</sch:assert>
            </sch:rule>
         </pattern>
         <rng:ref name="att.global.attributes"/>
         <rng:ref name="att.translatable.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">characterizes the element in some sense, using any convenient classification scheme or typology.
Suggested values include: 1] deprecationInfo (deprecation information)</a:documentation>
               <rng:choice>
                  <rng:value>deprecationInfo</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(deprecation
          information) This element describes why or how its parent element is being deprecated, typically including recommendations for alternate encoding.</a:documentation>
                  <data type="token">
                     <rng:param name="pattern">[^\p{C}\p{Z}]+</rng:param>
                  </data>
               </rng:choice>
            </attribute>
         </optional>
         <empty/>
      </rng:element>
   </define>
   <define name="gloss">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="gloss">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(gloss) identifies a phrase or word used to provide a gloss or definition for some other word or phrase. [3.4.1. Terms and Glosses 22.4.1. Description of Components]</a:documentation>
         <oneOrMore>
            <rng:ref name="term"/>
         </oneOrMore>
         <rng:ref name="att.global.rendition.attribute.rend"/>
         <attribute name="xml:id">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(identifier) provides a unique identifier for the element bearing the attribute.</a:documentation>
            <data type="ID"/>
         </attribute>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Identifies the glossary item as being either a person, place, or term.</a:documentation>
            <rng:choice>
               <rng:value>person</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <rng:value>place</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <rng:value>term</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <rng:value>text</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            </rng:choice>
         </attribute>
         <optional>
            <attribute name="mode">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Used to identify terms in the glossary that may not be applicable to every instance in the text. A <code xmlns="http://www.w3.org/1999/xhtml">&lt;gloss&gt;</code> element with the mode "match" will match with all instances of that string in the text unless they are marked with &lt;term type="ignore"&gt;. A term in the glossary with the mode "marked" will only link to terms with the <code xmlns="http://www.w3.org/1999/xhtml">&lt;term&gt;</code> element.</a:documentation>
               <rng:choice>
                  <rng:value>match</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Default; all instances are matched normally unless blocked with @type="ignore", or similar.</a:documentation>
                  <rng:value>marked</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">All instances are to be marked with <code xmlns="http://www.w3.org/1999/xhtml">&lt;term&gt;</code> tags.</a:documentation>
                  <rng:value>surfeit</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Glossary entry is to be ignored in the publication, and the combined glossary, but included in the translation memory.</a:documentation>
               </rng:choice>
            </attribute>
         </optional>
         <empty/>
      </rng:element>
   </define>
   <define name="term">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="term">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(term) contains a single-word, multi-word, or symbolic designation which is regarded as a technical term. [3.4.1. Terms and Glosses]</a:documentation>
         <rng:ref name="macro.phraseSeq"/>
         <rng:ref name="att.global.attribute.xmllang"/>
         <rng:ref name="att.global.rendition.attribute.rend"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Identifies the term in terms of its relation to the glossary, or its function within the glossary. See more detail on how to apply this type attribute in Appendix: .</a:documentation>
               <rng:choice>
                  <rng:value>ignore</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>definition</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>alternative</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>semanticReconstruction</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>transliterationReconstruction</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>sourceAttested</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>person-interlocutor</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>person-main-speaker</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>person-topical</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>main-subject</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>secondary-subject</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               </rng:choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="ref">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(reference) For the <code xmlns="http://www.w3.org/1999/xhtml">&lt;term&gt;</code> element, the @ref attribute should be used when there are three or more glossary entries with the exact same string and the instances of the term within the body of the text need to be distinguished. In such cases, the <code xmlns="http://www.w3.org/1999/xhtml">&lt;gloss&gt;</code> entry should be <code xmlns="http://www.w3.org/1999/xhtml">@mode</code>="marked".</a:documentation>
               <list>
                  <oneOrMore>
                     <data type="anyURI">
                        <rng:param name="pattern">\S+</rng:param>
                     </data>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <optional>
            <attribute name="status">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Identifies whether the term has been verified in the source or according to an authoritative dictionary. Tibetan may be verified by markup using the python script for checking the TEI against the eKangyur OR by checking it against the eKangyur in the glossary editing tool. Sanskrit may be verified either during markup or post-markup by a Sanskrit specialist in editorial.</a:documentation>
               <rng:choice>
                  <rng:value>verified</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               </rng:choice>
            </attribute>
         </optional>
         <empty/>
      </rng:element>
   </define>
   <define name="name">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="name">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(name, proper noun) contains a proper noun or noun phrase. [3.6.1. Referring Strings]</a:documentation>
         <text/>
      </rng:element>
   </define>
   <define name="date">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="date">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(date) contains a date in any format. [3.6.4. Dates and Times 2.2.4. Publication, Distribution, Licensing, etc. 2.6. The Revision Description 3.12.2.4. Imprint, Size of a Document, and Reprint Information 15.2.3. The Setting Description 13.4. Dates]</a:documentation>
         <text/>
         <rng:ref name="att.canonical.attributes"/>
         <empty/>
      </rng:element>
   </define>
   <define name="abbr">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="abbr">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(abbreviation) contains an abbreviation of any sort. [3.6.5. Abbreviations and Their Expansions]</a:documentation>
         <oneOrMore>
            <rng:choice>
               <text/>
               <rng:ref name="hi"/>
            </rng:choice>
         </oneOrMore>
      </rng:element>
   </define>
   <define name="expan">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="expan">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(expansion) contains the expansion of an abbreviation. [3.6.5. Abbreviations and Their Expansions]</a:documentation>
         <rng:ref name="macro.phraseSeq"/>
      </rng:element>
   </define>
   <define name="ptr">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="ptr">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(pointer) defines a pointer to another location. [3.7. Simple Links and Cross-References 16.1. Links]</a:documentation>
         <text/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="NewODD-ptr-ptrAtts-constraint-report-3">
            <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron" context="tei:ptr">
               <report test="@target and @cRef">Only one of the
attributes @target and @cRef may be supplied on <name/>.</report>
            </sch:rule>
         </pattern>
         <rng:ref name="att.internetMedia.attributes"/>
         <attribute name="target">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies the destination of the reference by supplying one or more URI References</a:documentation>
            <list>
               <oneOrMore>
                  <data type="anyURI">
                     <rng:param name="pattern">\S+</rng:param>
                  </data>
               </oneOrMore>
            </list>
         </attribute>
         <empty/>
      </rng:element>
   </define>
   <define name="ref">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="ref">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(reference) defines a reference to another location, possibly modified by additional text or comment. [3.7. Simple Links and Cross-References 16.1. Links]</a:documentation>
         <rng:ref name="macro.paraContent"/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="NewODD-ref-refAtts-constraint-report-4">
            <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron" context="tei:ref">
               <report test="@target and @cRef">Only one of the
	attributes @target' and @cRef' may be supplied on <name/>
               </report>
            </sch:rule>
         </pattern>
         <rng:ref name="att.cReferencing.attributes"/>
         <rng:ref name="att.global.attribute.xmllang"/>
         <rng:ref name="att.global.attribute.xmlid"/>
         <rng:ref name="att.internetMedia.attributes"/>
         <rng:ref name="att.pointing.attributes"/>
         <optional>
            <attribute name="rend">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(rendition) Identifies how this reference will be displayed in the reading room.</a:documentation>
               <list>
                  <oneOrMore>
                     <data type="token">
                        <rng:param name="pattern">[^\p{C}\p{Z}]+</rng:param>
                     </data>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Identifies the type of a reference</a:documentation>
               <rng:choice>
                  <rng:value>bampo</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>sanskrit</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>chinese</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>volume</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>folio</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               </rng:choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="key">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Identifies the Toh. number of the text to which a folio reference is linked to. </a:documentation>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="work">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Identifies which work the reference is referring to.</a:documentation>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="page">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Specifies which page index for the linked eKangyur when the index is irregular.</a:documentation>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="inst">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Provides instructions (typically just the folio number) for finding the URL of the specific milestone to be linked when the target of a pending unpublished text is published.</a:documentation>
               <text/>
            </attribute>
         </optional>
         <empty/>
      </rng:element>
   </define>
   <define name="list">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="list">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(list) contains any sequence of items organized as a list. [3.8. Lists]</a:documentation>
         <rng:zeroOrMore>
            <rng:ref name="head"/>
         </rng:zeroOrMore>
         <oneOrMore>
            <rng:choice>
               <rng:ref name="item"/>
               <rng:ref name="p"/>
            </rng:choice>
         </oneOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="NewODD-list-gloss-list-must-have-labels-constraint-rule-4">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0" xmlns:sch="http://purl.oclc.org/dsdl/schematron" context="tei:list[@type='gloss']">
	              <sch:assert test="tei:label">The content of a "gloss" list should include a sequence of one or more pairs of a label element followed by an item element</sch:assert>
            </sch:rule>
         </pattern>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">These "type" values are used to identify the types of lists used in various places throughout the text: "glossary" is applied to the main list in the glossary, "abbreviation" is applied to the list of abbreviations within the notes section of the text. In addition to these two standard lists, the <code xmlns="http://www.w3.org/1999/xhtml">&lt;list&gt;</code> element is also used for any outlines, bulleted lists, or other lists that appear with the actual text itself. In this case, the type attribute with the value "section" or "bullet" should be used, and this will effect how the list is displayed by the parser. The type value "section" will render a list of sections, with some vertical spacing between each item in the list; this is ideal for lists where each item is a paragraph or longer. The type value "bullet" will render the list with indentation but without vertical spacing; this is ideal for short items with identifiers (it isn't necessary that there be an actual "bullet" before each item, this is just the name we are using to distinguish the type). The latter two list types may also be used in combination to create an outline. For instance, see the sample of XML markup below for <a xmlns="http://www.w3.org/1999/xhtml" href="http://read.84000-translate.org/translation/toh116.html#UT22084-051-004-45">Toh. 116</a>. Note, that an additionally the attribute @rend="dots", "numbers", or "letters" maybe used to create a list with an automatically generated prefix (bullet, numbers, or letters respectively) and a hanging indent(see example below). This can be done in combination with a @type="section" list or a @type="bullet" list. For further information on outlines see Appendix B Topics: .</a:documentation>
            <rng:choice>
               <rng:value>glossary</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <rng:value>abbreviations</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <rng:value>section</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <rng:value>bullet</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            </rng:choice>
         </attribute>
         <optional>
            <attribute name="rend">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Renders further changes to the display of lists in HTML and other formats.</a:documentation>
               <rng:choice>
                  <rng:value>dots</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Items in the list will have a generated bullet dot and hanging indent (may be used in combination with @type="section" or @type="bullet").</a:documentation>
                  <rng:value>numbers</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Items in the list will have a generated number prefixes and hanging indent (may be used in combination with @type="section" or @type="bullet").</a:documentation>
                  <rng:value>letters</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Items in the list will have a generated letter prefixes and hanging indent (may be used in combination with @type="section" or @type="bullet").</a:documentation>
               </rng:choice>
            </attribute>
         </optional>
         <empty/>
      </rng:element>
   </define>
   <define name="item">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="item">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(item) contains one component of a list. [3.8. Lists 2.6. The Revision Description]</a:documentation>
         <rng:ref name="macro.specialPara"/>
         <rng:ref name="att.global.attribute.xmlid"/>
         <rng:ref name="att.global.rendition.attribute.rend"/>
         <empty/>
      </rng:element>
   </define>
   <define name="label">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="label">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(label) contains any label or heading used to identify part of a text.  [3.8. Lists]</a:documentation>
         <rng:ref name="macro.phraseSeq"/>
         <optional>
            <attribute name="tid">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <text/>
            </attribute>
         </optional>
         <empty/>
      </rng:element>
   </define>
   <define name="head">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="head">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(heading) contains any type of heading, for example the title of a section, or the heading of a list, glossary, manuscript description, etc. [4.2.1. Headings and Trailers]</a:documentation>
         <oneOrMore>
            <rng:choice>
               <text/>
               <rng:ref name="foreign"/>
               <rng:ref name="note"/>
               <rng:ref name="title"/>
               <rng:ref name="distinct"/>
            </rng:choice>
         </oneOrMore>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">classifies the head according to the 84000 typology.</a:documentation>
               <rng:choice>
                  <rng:value>summary</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>introduction</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>appendix</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>acknowledgment</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>abbreviations</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>description</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>table</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>nonStructuralBreak</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A heading that does not show a structural division and is not represented by a new parent <code xmlns="http://www.w3.org/1999/xhtml">&lt;div&gt;</code> element. Note that this will be displayed with a slightly smaller font than the regular heading and be held in square brackets. To display the heading in the same manner as the other headings without brackets, leave the <code xmlns="http://www.w3.org/1999/xhtml">&lt;head&gt;</code> elemant without a type attribute.</a:documentation>
                  <rng:value>translation</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>titleHon</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Honorifics before the title proper, e.g., "The Noble Mahāyāna Sūtra..."</a:documentation>
                  <rng:value>titleMain</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>sub</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">subtitle</a:documentation>
                  <rng:value>author</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">If author is to be named in the heading, as with a preface for instance (see toh 113).</a:documentation>
                  <rng:value>notes</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>section</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>chapter</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>list</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>listBibl</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>chapterTitle</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>prologue</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>preface</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>homage</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               </rng:choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="tid">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="key">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <text/>
            </attribute>
         </optional>
         <empty/>
      </rng:element>
   </define>
   <define name="note">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="note">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(note) contains a note or annotation. [3.9.1. Notes and Simple Annotation 2.2.6. The Notes Statement 3.12.2.8. Notes and Statement of Language 9.3.5.4. Notes within Entries]</a:documentation>
         <rng:zeroOrMore>
            <rng:choice>
               <rng:ref name="macro.specialPara"/>
               <rng:ref name="hi"/>
               <rng:ref name="mantra"/>
            </rng:choice>
         </rng:zeroOrMore>
         <rng:ref name="att.global.attributes"/>
         <optional>
            <attribute name="place">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies where this item is placed.</a:documentation>
               <list>
                  <oneOrMore>
                     <rng:choice>
                        <rng:value>end</rng:value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                     </rng:choice>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="index">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Gives the number of the note.</a:documentation>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="date-time">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="user">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="update">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="import">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <text/>
            </attribute>
         </optional>
         <optional>
            <attribute name="value">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <text/>
            </attribute>
         </optional>
         <empty/>
      </rng:element>
   </define>
   <define name="media">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="media">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">indicates the location of any form of external media such as an audio or video clip etc. [3.10. Graphics and Other Non-textual Components]</a:documentation>
         <rng:zeroOrMore>
            <rng:ref name="model.descLike"/>
         </rng:zeroOrMore>
         <rng:ref name="att.typed.attributes"/>
         <rng:ref name="att.global.attributes"/>
         <rng:ref name="att.timed.attributes"/>
         <attribute name="mimeType">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <rng:choice>
               <rng:value>audio/mpeg</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <rng:value>image/jpeg</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <rng:value>image/png</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            </rng:choice>
         </attribute>
         <attribute name="url">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(uniform resource locator) Indicate the url, "/audio/..." of the audio file from the folder db/apps/84000-data/audio/</a:documentation>
            <data type="anyURI">
               <rng:param name="pattern">\S+</rng:param>
            </data>
         </attribute>
         <empty/>
      </rng:element>
   </define>
   <define name="graphic">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="graphic">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(graphic) indicates the location of a graphic or illustration, either forming part of a text, or providing an image of it. [3.10. Graphics and Other Non-textual Components 11.1. Digital Facsimiles]</a:documentation>
         <empty/>
         <rng:ref name="att.media.attributes"/>
         <rng:ref name="att.resourced.attributes"/>
         <rng:ref name="att.typed.attributes"/>
         <empty/>
      </rng:element>
   </define>
   <define name="milestone">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="milestone">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(milestone) marks a boundary point separating any kind of section of a text, typically but not necessarily indicating a point at which some part of a standard reference system changes, where the change is not represented by a structural element. [3.11.3. Milestone
Elements]</a:documentation>
         <empty/>
         <rng:ref name="att.global.attribute.xmllang"/>
         <rng:ref name="att.global.rendition.attribute.rend"/>
         <attribute name="xml:id">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(identifier) provides a unique identifier for the element bearing the attribute.</a:documentation>
            <data type="ID"/>
         </attribute>
         <attribute name="unit">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">provides a conventional name for the kind of section changing at this milestone.</a:documentation>
            <rng:choice>
               <rng:value>chunk</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            </rng:choice>
         </attribute>
         <empty/>
      </rng:element>
   </define>
   <define name="lb">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="lb">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(line beginning) marks the beginning of a new (typographic) line in some edition or version of a text. [3.11.3. Milestone
Elements 7.2.5. Speech Contents]</a:documentation>
         <rng:empty/>
      </rng:element>
   </define>
   <define name="author">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="author">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(author) in a bibliographic reference, contains the name(s) of an author, personal or corporate, of a work; for example in the same form as that provided by a recognized bibliographic name authority. [3.12.2.2. Titles, Authors, and Editors 2.2.1. The Title Statement]</a:documentation>
         <rng:ref name="macro.phraseSeq"/>
         <rng:ref name="att.global.attribute.xmlid"/>
         <rng:ref name="att.global.rendition.attribute.rend"/>
         <optional>
            <attribute name="xml:lang">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <rng:choice>
                  <rng:value>Bo-Ltn</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Tibetan written in Roman script (Wylie or otherwise)</a:documentation>
                  <rng:value>Gu-Ltn</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Gujarti written in Roman script</a:documentation>
