               </rng:zeroOrMore>
               <rng:element name="date">
                  <data type="date"/>
               </rng:element>
            </group>
            <group>
               <rng:zeroOrMore>
                  <rng:element name="publisher">
                     <oneOrMore>
                        <rng:ref name="name"/>
                     </oneOrMore>
                  </rng:element>
               </rng:zeroOrMore>
               <oneOrMore>
                  <rng:choice>
                     <rng:element name="idno">
                        <attribute name="id" ns="http://www.w3.org/XML/1998/namespace">
                           <data type="ID"/>
                        </attribute>
                     </rng:element>
                     <rng:element name="idno">
                        <attribute name="kb-id" ns="http://read.84000.co/ns/1.0">
