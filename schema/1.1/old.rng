   <define name="publicationStmt">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="publicationStmt">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(publication statement) groups information concerning the publication or distribution of an electronic or other text. [2.2.4. Publication, Distribution, Licensing, etc. 2.2. The File Description]</a:documentation>
         <rng:choice>
            <group>
               <rng:element name="publisher">
                  <oneOrMore>
                     <rng:choice>
                        <text/>
                        <optional>
                           <rng:ref name="name"/>
                        </optional>
                        <rng:zeroOrMore>
                           <rng:ref name="lb"/>
                        </rng:zeroOrMore>
                        <rng:zeroOrMore>
                           <rng:ref name="emph"/>
                        </rng:zeroOrMore>
                     </rng:choice>
                  </oneOrMore>
               </rng:element>
               <rng:element name="availability">
                  <rng:zeroOrMore>
                     <rng:element name="p">
                        <attribute name="type">
                           <rng:value>tantricRestriction</rng:value>
                        </attribute>
                        <rng:zeroOrMore>
                           <rng:choice>
                              <text/>
                              <rng:ref name="emph"/>
                           </rng:choice>
                        </rng:zeroOrMore>
                     </rng:element>
                  </rng:zeroOrMore>
                  <rng:ref name="licence"/>
               </rng:element>
               <rng:element name="idno">
                  <attribute name="id" ns="http://www.w3.org/XML/1998/namespace">
