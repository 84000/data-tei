                     </rng:element>
                     <rng:element name="date">
                        <optional>
                           <data type="date"/>
                        </optional>
                     </rng:element>
                  </rng:choice>
               </rng:zeroOrMore>
            </group>
         </rng:choice>
         <attribute name="status">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Identifies what stage the text is in preparation for publication.</a:documentation>
            <rng:choice>
               <rng:value>1</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">All formats published, including App</a:documentation>
               <rng:value>1.a</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Translations completed and published, awaiting publication on App</a:documentation>
               <rng:value>2</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Markedup, awaiting final proofing</a:documentation>
               <rng:value>2.a</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Markup in process</a:documentation>
               <rng:value>2.b</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Awaiting markup</a:documentation>
               <rng:value>2.c</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Awaiting copyeditor's OK for markup</a:documentation>
               <rng:value>2.d</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Copyediting complete. Preparing for markup</a:documentation>
               <rng:value>2.e</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Being copyedited</a:documentation>
               <rng:value>2.f</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Review complete. Awaiting copyediting</a:documentation>
               <rng:value>2.g</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">In Editorial review</a:documentation>
               <rng:value>2.h</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Awaiting review</a:documentation>
               <rng:value>3</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Current translation projects</a:documentation>
               <rng:value>4</rng:value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Application pending</a:documentation>
               <rng:value/>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            </rng:choice>
         </attribute>
         <empty/>
      </rng:element>
   </define>
   <define name="idno">
