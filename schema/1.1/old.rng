   <define name="lg">
      <rng:element xmlns:rng="http://relaxng.org/ns/structure/1.0" name="lg">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(line group) contains one or more verse lines functioning as a formal unit, e.g. a stanza, refrain, verse paragraph, etc. [3.13.1. Core Tags for Verse 3.13. Passages of Verse or Drama 7.2.5. Speech Contents]</a:documentation>
         <oneOrMore>
            <rng:choice>
               <rng:ref name="l"/>
               <rng:ref name="milestone"/>
               <rng:ref name="mantra"/>
               <rng:ref name="modPassage"/>
            </rng:choice>
         </oneOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="NewODD-lg-atleast1oflggapl-constraint-assert-5">
            <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron" context="tei:lg">
               <sch:assert xmlns="http://www.tei-c.org/ns/1.0" test="count(descendant::tei:lg|descendant::tei:l|descendant::tei:gap) &gt; 0">An lg element
        must contain at least one child l, lg, or gap element.</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron" id="NewODD-lg-abstractModel-structure-lg-in-l-constraint-report-6">
            <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron" context="tei:lg">
               <sch:report xmlns="http://www.tei-c.org/ns/1.0" test="ancestor::tei:l[not(.//tei:note//tei:lg[. = current()])]">
        Abstract model violation: Lines may not contain line groups.
      </sch:report>
            </sch:rule>
         </pattern>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Used to denote specific types of verses such as "sdom" or "bar_sdom" indexes and so forth.</a:documentation>
               <rng:choice>
                  <rng:value>sdom</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>bar_sdom</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
                  <rng:value>spyi_sdom</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               </rng:choice>
            </attribute>
         </optional>
         <attribute name="tid">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Numbers paragraph and line groups in a particular text.</a:documentation>
            <text/>
         </attribute>
         <optional>
            <attribute name="rend">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <rng:choice>
                  <rng:value>mantra</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">This <code xmlns="http://www.w3.org/1999/xhtml">&lt;lg&gt;</code> element contains a mantra. The mantra has been formally and clearly declared and so will set apart with indentation and vertical spacing in the 84000 web display. Not all <code xmlns="http://www.w3.org/1999/xhtml">&lt;lg&gt;</code> elements that contain mantras need to be rendered in this way, but generally we do so, when they are formally and clearly declared in the text.</a:documentation>
                  <rng:value>ignoreGlossary</rng:value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Glossary terms found in this line group will not be linked to the glossary in the 84000 Reading Room.</a:documentation>
               </rng:choice>
            </attribute>
         </optional>
         <empty/>
      </rng:element>
   </define>
   <define name="teiHeader">
